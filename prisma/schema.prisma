datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  owner      String
  ownerEmail String
  title      String
  roomNo     Int
  mobile     String
  Image      String?
  price      Int
  createdAt  DateTime @default(now())
}
